<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<xwork>
	<include file="webwork-default.xml"/>
	<include file="xwork-intercept.xml"/>
	<package name="wfRelate" extends="webwork-default,intercept">
		<action name="WfRelate" class="gnwf.ww.json.WfRelateAction">
			<interceptor-ref name="permissionStack"/>
			<result name="nologgedin">/jsp/msg.jsp</result>
			<result name="nopermission">/jsp/msg.jsp</result>
			<result name="initializes">/liger/gnwf/WfRelate/list.jsp</result>
			<result name="success">/jsp/app.jsp</result>
			<result name="message">/jsp/msg.jsp</result>
			<result name="add">/liger/gnwf/WfRelate/add.jsp</result>
			<result name="edit">/liger/gnwf/WfRelate/edit.jsp</result>
			<result name="view">/liger/gnwf/WfRelate/view.jsp</result>
			<result name="list">/jsp/app.jsp</result>
			<result name="upload">/liger/gnwf/WfRelate/upload.jsp</result>
			<result name="print">/liger/gnwf/WfRelate/print.jsp</result>
			<result name="excel" type="excel">
				<param name="filename">wfRelatexls</param>
			</result>
			<result name="error">/jsp/msg.jsp</result>
		</action>
	</package>
</xwork>